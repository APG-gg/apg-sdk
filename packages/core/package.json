    "@draft-js-plugins/mention": "^5.2.2",
    "@splidejs/react-splide": "^0.7.12",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-highlight-words": "^0.16.7",
    "@types/react-mentions": "^4.1.13",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "draft-js": "^0.11.7",
    "html2canvas": "^1.4.1",
    "moment": "^2.29.4",
    "next-translate": "^2.6.0",
    "rc-mentions": "^2.9.1",
    "rc-picker": "^3.14.6",
